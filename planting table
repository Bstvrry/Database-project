CREATE TABLE PlantingTable ( TreeID INT PRIMARY KEY, Location VARCHAR(50), Growth_Rate DECIMAL, SpeciesID INT, FOREIGN KEY (SpeciesID) REFERENCES SpeciesTable(SpeciesID) );
